import java.util.Scanner;

class twodarray{
     public static void main(String args[])
	 {
	  int i,j; 
	  int p[][] = new int[3][4];
      Scanner obj = new Scanner(System.in);
      System.out.println("Enter Elements of Array : ");
      for(i=0;i<3;i++){
		  for(j=0;j<4;j++){
			  p[i][j] = obj.nextInt();
		  }
	  }
	  System.out.println("Your Array Elements are : ");
	  for(i=0;i<3;i++){
		  for(j=0;j<4;j++){
			  System.out.print(p[i][j] + " "); 
		  }
	  }
	 }
}